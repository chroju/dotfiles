{
    "workbench.colorTheme": "Iceberg",
    "editor.minimap.enabled": false,
    "editor.wordWrap": "on",
    "files.insertFinalNewline": true,
    "editor.fontFamily": "'Cica', 'Ricty Diminished Discord', Menlo, Monaco, 'Courier New', monospace",
    "vim.easymotion": true,
    "vim.visualstar": true,
    "vim.useSystemClipboard": true,
    "vim.hlsearch": true,
    "vim.statusBarColors": {
        "normal": [
            "#8ac6f2",
            "#444444"
        ],
        "insert": [
            "#95e454",
            "#444444"
        ],
        "visual": [
            "#f2c68a",
            "#444444"
        ],
        "visualline": [
            "#f2c68a",
            "#444444"
        ],
        "visualblock": [
            "#f2c68a",
            "#444444"
        ],
        "replace": [
            "#e5786d",
            "#444444"
        ]
    },
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "leader",
                "h"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "l"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "g",
                "j"
            ],
            "after": [
                "j"
            ]
        },
        {
            "before": [
                "g",
                "k"
            ],
            "after": [
                "k"
            ]
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "leader",
                "h"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "leader",
                "l"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "j"
            ],
            "after": [
                "g",
                "j"
            ]
        },
        {
            "before": [
                "k"
            ],
            "after": [
                "g",
                "k"
            ]
        },
        {
            "before": [
                "g",
                "j"
            ],
            "after": [
                "j"
            ]
        },
        {
            "before": [
                "g",
                "k"
            ],
            "after": [
                "k"
            ]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "leader",
                ":"
            ],
            "commands": [
                "workbench.action.showCommands",
            ]
        }
    ],
    "workbench.colorCustomizations": {
        "statusBar.background": "#8ac6f2",
        "statusBar.noFolderBackground": "#8ac6f2",
        "statusBar.debuggingBackground": "#8ac6f2",
        "statusBar.foreground": "#444444",
        "sideBar.foreground": "#cccccc"
    },
    "workbench.activityBar.visible": true,
    "explorer.confirmDragAndDrop": false,
    "git.autofetch": true,
    "go.useLanguageServer": true,
    "editor.suggestSelection": "first",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 10000,
    "latex-workshop.message.log.show": true,
    "latex-workshop.message.badbox.show": true,
    "latex-workshop.latex.autoBuild.run": "onFileChange",
    "latex-workshop.latex.recipes": [
        {
            "name": "ptex2pdf",
            "tools": [
                "ptex2pdf"
            ]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "ptex2pdf",
            "command": "ptex2pdf",
            "args": [
                "-l",
                "-ot",
                "-kanji=utf8 -synctex=1",
                "%DOC%"
            ]
        }
    ],
    "latex-workshop.view.pdf.viewer": "tab",
    "python.jediEnabled": false,
    "[go]": {
        "editor.snippetSuggestions": "none",
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "[terraform]": {
        "editor.formatOnSave": true
    },
    "terminal.integrated.rendererType": "dom",
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 500,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
    "terraform.languageServer": {
        "enabled": true,
        "args": []
    },
    "editor.fontSize": 16,
    "editor.formatOnSave": true,
    "terminal.integrated.fontSize": 16,
    "window.zoomLevel": 0,
    "python.formatting.provider": "autopep8",
    "aws.samcli.location": "/usr/local/bin/sam"
}
